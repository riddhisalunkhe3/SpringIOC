<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans-4.3.xsd">
                            
 <bean class="com.springIOC.Task.Tree" name="t1">
    <property name="treeId" value="101"/>
    <property name="treeName" value="Guava tree"/>
 </bean>
 <bean class="com.springIOC.Task.Bird" name="b1">
    <property name="birdId" value="1"/>
    <property name="birdName" value="Parrot"/>
    <property name="tree">
               <ref bean="t1"/>
             </property>
 </bean> 
   
